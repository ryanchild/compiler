// meaningless test program for fake programming language

integer function main()
  integer a
  string c
  integer e
  float f
  integer i
  integer j
  string s
  boolean b
  global float gf

  float array[10]
  integer arr2[20]

  // nested global function
  global float function floatfunc(boolean a, boolean b)
    float ret
  begin
    if a == b then
      ret := 0.123
    else
      ret := -583.13
    end if

    floatfunc := ret
  end function

  boolean function emptyfunc()
  begin
  end function
 
begin
  a:=getInt() 
  f:=getFloat()
  b := true
  b:=getBool()

  c := "Hello world!?09;:.,.."
  c := getString()
  a:=putString(c)

  // function calls have to be part of an assignment statement?
  a := putString("a: ")
  a := putInt(a)
  a := putString("f: ")
  a := putFloat(f)
  a := putString("b: ")
  a := putBool(b)

  // test relations
  if b==2 then
    b := b+1
  else 
    // nested if/else
    if b<1 then
      b := b/2
    end if
  end if

  if b<1 then
    b := b/2
  end if

  if b>3 then
    b := b*3
  end if

  if b != 10 then
    b := b - 1
  end if

  if b <= 100 then
    b := b+1
  end if

  if b>=200.35 then
    b := b*10
  end if

  // test loops
  for i:=1 i<10
    a := a+1

    // nested for loop
    for j:=0 j<i
      b := b-1
    end for
  end for

  gf := 1.234

  a := 1
  array[a] := 1
  array[2] := -gf - -4.321

  d := e | f
  d := d & f
  e := not d

  d := a & b
  e := a | b
  f := not b

  main:=b
end function
